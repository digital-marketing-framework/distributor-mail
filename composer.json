{
  "name": "digital-marketing-framework/distributor-mail",
  "type": "library",
  "description": "Digital Marketing Framework - Distributor - Mail",
  "keywords": [
    "framework",
    "form",
    "data",
    "submission",
    "relay",
    "email",
    "mail"
  ],
  "license": "GPL-2.0-or-later",
  "homepage": "https://www.mediatis.de",
  "authors": [
    {
      "email": "info@mediatis.de",
      "homepage": "https://www.mediatis.de",
      "name": "Mediatis AG",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:digital-marketing-framework/distributor-core.git",
      "only": [
        "digital-marketing-framework/distributor-core"
      ]
    },
    {
      "type": "vcs",
      "url": "git@github.com:digital-marketing-framework/core.git",
      "only": [
        "digital-marketing-framework/core"
      ]
    }
  ],
  "require": {
    "php": "^8.1",
    "digital-marketing-framework/distributor-core": "dev-main"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "DigitalMarketingFramework\\Distributor\\Mail\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DigitalMarketingFramework\\Distributor\\Mail\\Tests\\": "tests/",
      "DigitalMarketingFramework\\Distributor\\Request\\Tests\\": "vendor/digital-marketing-framework/distributor-request/tests/",
      "DigitalMarketingFramework\\Distributor\\Core\\Tests\\": "vendor/digital-marketing-framework/distributor-core/tests/",
      "DigitalMarketingFramework\\Core\\Tests\\": "vendor/digital-marketing-framework/core/tests/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit --bootstrap vendor/autoload.php tests/",
    "testdox": "./vendor/bin/phpunit --bootstrap vendor/autoload.php --testdox --colors=always tests/"
  }
}
